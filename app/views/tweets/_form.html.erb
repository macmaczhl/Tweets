<%= form_for(tweet) do |f| %>
  <div class="field form-group">
    <%= f.text_area :text, size: '30x5', class: 'form-control' %>
  </div>

  <div class="field form-part">
    <%= f.file_field :image %>
  </div>

  <div class="actions form-part tweet-btn">
    <%= f.submit "Tweet!", class: "btn btn-default" %>
  </div>
<% end %>
